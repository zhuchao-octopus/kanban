<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="52dp"
    android:background="@android:color/white"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:orientation="horizontal">
        <View
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:background="@android:color/black"/>
    <TextView
        android:id="@+id/tv_order"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:text="AKWD-201812-00001"
        android:textColor="#000000"
        android:textSize="20sp"
        android:gravity="center"/>
    <View
        android:layout_width="2dp"
        android:layout_height="match_parent"
        android:background="@android:color/black"/>
    <TextView
        android:id="@+id/tv_customer"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:text="ZLXM"
        android:textColor="#000000"
        android:textSize="20sp"
        android:gravity="center"/>
    <View
        android:layout_width="2dp"
        android:layout_height="match_parent"
        android:background="@android:color/black"/>
    <TextView
        android:id="@+id/tv_type"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:text="MXQ-4K"
        android:textColor="#000000"
        android:textSize="20sp"
        android:gravity="center"/>
    <View
        android:layout_width="2dp"
        android:layout_height="match_parent"
        android:background="@android:color/black"/>
    <TextView
        android:id="@+id/tv_model"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="2"
        android:text="RK32291+86051 3DFLASH"
        android:textColor="#000000"
        android:textSize="20sp"
        android:gravity="center"/>
    <View
        android:layout_width="2dp"
        android:layout_height="match_parent"
        android:background="@android:color/black"/>
    <TextView
        android:id="@+id/tv_order_number"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:text="300"
        android:textColor="#000000"
        android:textSize="20sp"
        android:gravity="center"/>
    <View
        android:layout_width="2dp"
        android:layout_height="match_parent"
        android:background="@android:color/black"/>
    <TextView
        android:id="@+id/tv_production_number"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:text="32"
        android:textColor="#000000"
        android:textSize="20sp"
        android:gravity="center"/>
    <View
        android:layout_width="2dp"
        android:layout_height="match_parent"
        android:background="@android:color/black"/>
    <TextView
        android:id="@+id/tv_production_input_number"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:text="10000"
        android:textColor="#000000"
        android:textSize="20sp"
        android:gravity="center"/>
    <View
        android:layout_width="2dp"
        android:layout_height="match_parent"
        android:background="@android:color/black"/>
    <TextView
        android:id="@+id/tv_production_output_number"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:text="10000"
        android:textColor="#000000"
        android:textSize="20sp"
        android:gravity="center"/>
    <View
        android:layout_width="2dp"
        android:layout_height="match_parent"
        android:background="@android:color/black"/>
    <TextView
        android:id="@+id/tv_cumulative_input_number"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:text="90"
        android:textColor="#000000"
        android:textSize="20sp"
        android:gravity="center"/>
    <View
        android:layout_width="2dp"
        android:layout_height="match_parent"
        android:background="@android:color/black"/>
    <TextView
        android:id="@+id/tv_cumulative_output_number"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:text="90"
        android:textColor="#000000"
        android:textSize="20sp"
        android:gravity="center"/>
    <View
        android:layout_width="2dp"
        android:layout_height="match_parent"
        android:background="@android:color/black"/>
    <TextView
        android:id="@+id/tv_owe"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:text="910"
        android:textColor="#000000"
        android:textSize="20sp"
        android:gravity="center"/>
    <View
        android:layout_width="2dp"
        android:layout_height="match_parent"
        android:background="@android:color/black"/>
    <TextView
        android:id="@+id/tv_remark"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_weight="0.9"
        android:text="æ— "
        android:textColor="#000000"
        android:textSize="20sp"
        android:gravity="center"/>
        <View
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:background="@android:color/black"/>
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:background="@android:color/black"/>
</LinearLayout>
